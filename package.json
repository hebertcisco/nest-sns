{
  "name": "nest-sns",
  "version": "0.3.9",
  "description": "Amazon Simple Notification Service module ðŸŒ",
  "author": "Hebert Cisco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"lib/**/*.(js|ts)\"",
    "lint": "eslint lib --ext .js,.ts",
    "lint:fix": "eslint lib --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.300.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "9.4.3"
  },
  "devDependencies": {
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/testing": "^9.3.12",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.51.0",
    "eslint-plugin-jest": "27.4.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-cucumber": "^3.0.1",
    "nest-sns": "^0.3.2",
    "prettier": "^3.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm",
    "arm64",
    "ia32",
    "!mips"
  ],
  "keywords": [
    "nest",
    "sns",
    "aws",
    "sns-publish",
    "sms",
    "email",
    "notification",
    "amazon",
    "simple",
    "aws"
  ],
  "license": "MIT",
  "url": "https://github.com/hebertcisco/nest-sns#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hebertcisco/nest-sns"
  }
}
