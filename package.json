{
  "name": "nest-sns",
  "version": "0.1.0",
  "description": "Amazon Simple Notification Service module 🌐",
  "author": "Hebert Cisco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"lib/**/*.(js|ts)\"",
    "lint": "eslint lib --ext .js,.ts",
    "lint:fix": "eslint lib --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.241.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "9.2.1"
  },
  "devDependencies": {
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-plugin-jest": "27.2.1",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "prettier": "^2.8.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.0.7",
    "rxjs": "7.8.0",
    "ts-jest": "28.0.7",
    "typescript": "4.9.4"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=16.16",
    "npm": ">=8.11"
  },
  "os": [
    "!darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "!arm",
    "!mips"
  ],
  "keywords": [
    "nest",
    "sns",
    "aws",
    "sns-publish",
    "sms",
    "email",
    "notification",
    "amazon",
    "simple",
    "aws"
  ],
  "license": "MIT",
  "url": "https://github.com/hebertcisco/nest-sns#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hebertcisco/nest-sns"
  }
}
